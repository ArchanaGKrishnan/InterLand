.container-ex {
    position: relative;
    width: 100%;
}

.overlay {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    height: 100%;
    width: 100%;
    opacity: 0;
    transition: .3s ease;
    /* background-color: red; */
    background: -webkit-gradient(linear, left top, right top, from(#b3070e), to(#0e044b));
    background-position: 50% 50%;
    background-size: 0% 100%;
    transition: background-size 1s, color .5s;
    background-repeat: no-repeat;
}

.container-ex:hover .overlay {
    opacity: 0.8;
    background-size: 100% 100%;
    background-color: unset;
    /* transition: background-size 1s 0s; */
}

.container-ex:hover .container-ex {
    background: unset;
}

